<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.appspot.AccentNijkerk.controller.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>com.appspot.AccentNijkerk.controller.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CompetentieLijstToevoegenServlet</servlet-name>
		<servlet-class>com.appspot.AccentNijkerk.controller.CompetentieLijstToevoegenServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CompetentieLijstToevoegenServlet</servlet-name>
		<url-pattern>/competentielijst-toevoegen</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LeerlingToevoegenServlet</servlet-name>
		<servlet-class>com.appspot.AccentNijkerk.controller.LeerlingToevoegenServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LeerlingToevoegenServlet</servlet-name>
		<url-pattern>/leerling-toevoegen</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MedewerkerToevoegenServlet</servlet-name>
		<servlet-class>com.appspot.AccentNijkerk.controller.MedewerkerToevoegenServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MedewerkerToevoegenServlet</servlet-name>
		<url-pattern>/medewerker-toevoegen</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>StageBedrijfToevoegenServlet</servlet-name>
		<servlet-class>com.appspot.AccentNijkerk.controller.StageBedrijfToevoegenServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StageBedrijfToevoegenServlet</servlet-name>
		<url-pattern>/stagebedrijf-toevoegen</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>StageToevoegenServlet</servlet-name>
		<servlet-class>com.appspot.AccentNijkerk.controller.StageToevoegenServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StageToevoegenServlet</servlet-name>
		<url-pattern>/stage-toevoegen</url-pattern>
	</servlet-mapping>
	<listener> 
		<listener-class>com.appspot.AccentNijkerk.listeners.ContextListener</listener-class>
	</listener>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>